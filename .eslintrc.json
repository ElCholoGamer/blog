{
	"plugins": ["@typescript-eslint"],
	"extends": ["next/core-web-vitals", "plugin:@typescript-eslint/recommended"],
	"rules": {
		"semi": "warn",
		"prefer-const": "error",
		"@typescript-eslint/no-unused-vars": "error",
		"@typescript-eslint/no-explicit-any": "error",
		"arrow-parens": "off"
	}
}
